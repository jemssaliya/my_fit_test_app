{
    "name": "emedia/oxygen",
    "description": "Foundation Laravel platform with a Dashboard and User handling.",
    "type": "project",
    "repositories": [
        {
            "type":"vcs",
            "url":"git@bitbucket.org:elegantmedia/multitenant-laravel.git"
        },
        {
            "type":"vcs",
            "url":"git@bitbucket.org:elegantmedia/lotus.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:elegantmedia/formation.git"
        },
        {
            "type":"vcs",
            "url":"git@bitbucket.org:elegantmedia/laravel-app-settings.git"
        },
        {
            "type":"vcs",
            "url":"git@bitbucket.org:elegantmedia/laravel-generators.git"
        },
        {
            "type":"vcs",
            "url":"git@bitbucket.org:elegantmedia/quickdata-laravel.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:elegantmedia/laravel-helpers.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:elegantmedia/php-helpers.git"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "^5.7",
        "silber/bouncer": "v1.0.0-rc.3",
        "emedia/quick-data-laravel": "~2.0",
        "emedia/generators": "~2.0",
        "emedia/lotus": "~1.0",
        "emedia/app-settings": "~1.0",
        "emedia/multi-tenant": "0.1.*",
        "doctrine/dbal": "^2.8",
        "cviebrock/eloquent-sluggable": "^4.6",
        "laravelcollective/html": "^5.4",
        "barryvdh/laravel-debugbar": "^3.2",
        "google/recaptcha": "^1.2",
        "webpatser/laravel-uuid": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "EMedia\\Oxygen\\OxygenServiceProvider"
            ]
        }
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Shane Perera",
            "email": "shane@elegantmedia.com.au"
        }
    ],
    "autoload": {
        "psr-4": {
            "EMedia\\Oxygen\\": "src/",
            "EMedia\\Oxygen\\Database\\": "database/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "LaravelDefaultFiles/app/",
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
